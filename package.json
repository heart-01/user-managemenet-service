{
  "name": "user-management-service",
  "version": "1.0.0",
  "main": "app.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "prestart": "npm run update:type && dotenv -e .env.prod -- npm run migrate && npm run seed",
    "start": "dotenv -e .env.prod -- node dist/app.js",
    "build": "prisma generate && tsc --build tsconfig.json",
    "debug": "dotenv -e .env.dev -- npm run migrate && npm run seed && tsc --build tsconfig.json && tsc-watch --onSuccess \"node --inspect=0.0.0.0:9229 ./dist/app.js\"",
    "dev": "ts-node-dev src/app.ts",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --config jest.unit.config.ts",
    "test:unit-cov": "jest --coverage --config jest.unit.config.ts",
    "test:integration": "dotenv -e .env.test -- npm run migrate && jest --config jest.integration.config.ts",
    "lint": "eslint . --ignore-path ./.gitignore --ext .ts",
    "eslint:fix": "eslint --fix",
    "prettier": "prettier \"src/**/*.{ts,json}\"",
    "format": "npm run prettier -- --write",
    "prebuild": "npm run prettier -- --list-different && npm run lint",
    "update:type": "prisma generate",
    "seed": "prisma db seed",
    "migrate:gen": "prisma migrate dev --create-only --name ",
    "migrate": "prisma migrate deploy"
  },
  "description": "",
  "dependencies": {
    "@casl/ability": "^6.7.2",
    "@casl/prisma": "^1.5.0",
    "@prisma/client": "^5.22.0",
    "@sendgrid/mail": "^8.1.4",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "express": "^4.21.1",
    "express-jsdoc-swagger": "^1.8.0",
    "express-rate-limit": "^7.4.1",
    "google-auth-library": "^9.14.2",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^11.0.2",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.8.4",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.0",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3"
  }
}
